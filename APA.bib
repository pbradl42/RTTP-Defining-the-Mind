@article{Murre:2015ix,
author = {Murre, Jaap M J and Dros, Joeri},
title = {{Replication and Analysis of Ebbinghaus{\textquoteright} Forgetting Curve}},
journal = {PLOS ONE},
year = {2015},
volume = {10},
number = {7},
pages = {e0120644},
month = jul,
publisher = {Public Library of Science},
doi = {10.1371/journal.pone.0120644},
language = {English},
rating = {0},
date-added = {2018-07-17T20:27:26GMT},
date-modified = {2018-07-17T20:27:27GMT},
abstract = {We present a successful replication of Ebbinghaus{\textquoteright} classic forgetting curve from 1880 based on the method of savings. One subject spent 70 hours learning lists and relearning them after 20 min, 1 hour, 9 hours, 1 day, 2 days, or 31 days. The results are similar to Ebbinghaus' original data. We analyze the effects of serial position on forgetting and investigate what mathematical equations present a good fit to the Ebbinghaus forgetting curve and its replications. We conclude that the Ebbinghaus forgetting curve has indeed been replicated and that it is not completely smooth but most probably shows a jump upwards starting at the 24 hour data point.},
url = {http://dx.plos.org/10.1371/journal.pone.0120644},
local-url = {file://localhost/Users/Peter/Dropbox%20(Honors%20@%20Ferris)/PapersDB/Library.papers3/Files/FF/FF2F8A37-232A-4907-87FB-E66952FA85A9.pdf},
